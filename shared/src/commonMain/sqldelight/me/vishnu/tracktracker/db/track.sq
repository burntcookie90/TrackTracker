CREATE TABLE track (
    id TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL UNIQUE,
    location TEXT NOT NULL,
    note_id INTEGER
);

selectAll:
SELECT *
FROM track;

insertNewTrack:
INSERT INTO track(id, name, location)
VALUES (?, ?, ?);

insertNewTrackWithNote:
INSERT INTO track(id, name, location, note_id)
VALUES (?, ?, ?, ?);